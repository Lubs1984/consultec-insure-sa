{
  "name": "@insureconsultec/api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:migrate": "prisma migrate dev --schema=src/infrastructure/database/prisma/schema.prisma",
    "db:deploy": "prisma migrate deploy --schema=src/infrastructure/database/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=src/infrastructure/database/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/infrastructure/database/prisma/schema.prisma",
    "db:seed": "tsx src/infrastructure/database/prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/rate-limit": "^10.2.0",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@aws-sdk/client-s3": "^3.716.0",
    "@aws-sdk/s3-request-presigner": "^3.716.0",
    "@insureconsultec/shared": "workspace:*",
    "@prisma/client": "^6.2.1",
    "bullmq": "^5.34.0",
    "fastify-plugin": "^5.0.1",
    "ioredis": "^5.4.2",
    "fastify": "^5.2.1",
    "bcryptjs": "^2.4.3",
    "pino": "^9.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.0",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
